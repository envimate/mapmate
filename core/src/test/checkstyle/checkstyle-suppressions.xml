<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.1//EN"
        "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">
<suppressions>
    <suppress checks="VisibilityModifier" files=".*AggregatedValidationException.java"/>
    <suppress checks="VisibilityModifier" files=".*UnrecognizedExceptionOccurredException.java"/>
    <suppress checks="VisibilityModifier" files=".*ValidationError.java"/>
    <suppress checks="VisibilityModifier" files=".*Injector.java"/>
    <suppress checks="IllegalCatchCheck" files="NamedMethodSerializationCPMethod.java"/>
    <suppress checks="IllegalCatchCheck" files="DataTransferObjectPackageScanner.java"/>
    <suppress checks="ParameterNumber" files="SerializedObjectWithPrimitives.java"/>
    <suppress checks="IllegalCatchCheck" files="Unmarshallers.java"/>
    <suppress checks="IllegalCatchCheck" files="Serializer.java"/>
    <suppress checks="IllegalCatchCheck" files="Deserializer|When"/>
    <suppress checks="MethodLength" files="Deserializer.java"/>
    <suppress checks="MethodLength" files="MapMateBuilder.java"/>
    <suppress checks="VisibilityModifier" files="MapMateBuilder.java"/>
    <suppress
            checks="MagicNumber|MethodName|IllegalCatch|LineLength|MethodLength|FinalParameters|FileLength|InnerTypeLast"
            files="DeserializerBuilderTest|DeserializerTest|SerializerTest|SerializerBuilderTest|CircularReferenceDetectorTest|Examples"/>
    <suppress checks="VisibilityModifier" files="AComplexValidatedType|ACyclicType|User"/>
    <suppress checks="MethodName|Regex" files="Examples"/>
    <suppress checks="ClassFanOutComplexityCheck" files="MapMateBuilder.java"/>
    <suppress checks="RedundantModifier" files="DefaultPackageScanner.java"/>

</suppressions>
